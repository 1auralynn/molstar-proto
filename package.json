{
  "name": "mol-star-proto",
  "version": "0.1.0",
  "description": "Comprehensive molecular library.",
  "scripts": {
    "lint": "./node_modules/.bin/tslint src/**/*.ts",
    "build": "./node_modules/.bin/tsc",
    "watch": "./node_modules/.bin/tsc -watch",
    "test": "./node_modules/.bin/jest",
    "script": "node build/node_modules/script.js",
    "download-dics": "./node_modules/.bin/download -o build/dics http://mmcif.wwpdb.org/dictionaries/ascii/mmcif_pdbx_v50.dic && ./node_modules/.bin/download -o build/dics http://mmcif.wwpdb.org/dictionaries/ascii/mmcif_ddl.dic"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "\\.ts$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "moduleDirectories": [
      "node_modules",
      "build/node_modules"
    ],
    "testRegex": "\\.spec\\.ts$"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/benchmark": "^1.0.31",
    "@types/express": "^4.11.0",
    "@types/jest": "^21.1.10",
    "@types/node": "^8.5.8",
    "@types/node-fetch": "^1.6.7",
    "benchmark": "^2.1.4",
    "download-cli": "^1.0.5",
    "jest": "^21.2.1",
    "rollup": "^0.50.1",
    "rollup-plugin-buble": "^0.16.0",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.0.2",
    "rollup-watch": "^4.3.1",
    "ts-jest": "^21.2.4",
    "tslint": "^5.9.1",
    "typescript": "^2.6.2",
    "uglify-js": "^3.3.7",
    "util.promisify": "^1.0.0"
  },
  "dependencies": {
    "express": "^4.16.2",
    "node-fetch": "^1.7.3"
  }
}
